# Morocco 2030 World Cup Backend Environment Variables

# Server Configuration
PORT=5000
NODE_ENV=development

# API Keys for External Services
YELP_API_KEY=your_yelp_fusion_api_key_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
RATEHAWK_API_KEY=your_ratehawk_api_key_here
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_API_SECRET=your_amadeus_api_secret_here
FOURSQUARE_API_KEY=your_foursquare_api_key_here

# Database Configuration (if using database)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=morocco_worldcup
DB_USER=postgres
DB_PASSWORD=your_password

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# JWT Secret (for authentication)
JWT_SECRET=your_jwt_secret_here

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:8081,http://localhost:4173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
