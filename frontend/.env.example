# Morocco 2030 World Cup Frontend Environment Variables
# Copy this file to .env.local and add your actual values

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:5001/api

# External API Keys (Optional - handled by backend)
# Only needed if you want to call APIs directly from frontend

# Google Places API (for restaurant and location discovery)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Yelp Fusion API (for restaurant data and reviews)
VITE_YELP_API_KEY=your_yelp_fusion_api_key_here

# RateHawk API (for hotel booking - free tier available)
VITE_RATEHAWK_API_KEY=your_ratehawk_api_key_here

# Amadeus API (for premium hotel services)
VITE_AMADEUS_API_KEY=your_amadeus_api_key_here
VITE_AMADEUS_API_SECRET=your_amadeus_api_secret_here

# Foursquare API (for location data)
VITE_FOURSQUARE_API_KEY=your_foursquare_api_key_here

# Development Configuration
VITE_NODE_ENV=development
VITE_DEBUG_MODE=true
